{
    "properties": {

        "formSchema": {
            "type":    "object",
            "options": {
                "label":       "Schema",
                "description": "The schema you want to render"
            }
        },

        "formValues": {
            "type":    "object",
            "options": {
                "label":       "Values",
                "description": "The values that will be used to populate schema."
            }
        },

        "enctype": {
            "type":    "string",
            "options": {
                "label": "Enctype",
                "value": "application/x-www-form-urlencoded"
            }
        },

        "method": {
            "type":    "select",
            "options": {
                "label":        "Method",
                "default":      "POST",
                "multiOptions": {
                    "POST": "POST",
                    "GET":  "GET"

                }

            }
        },

        "maxCols": {
            "type":    "number",
            "options": {
                "label":       "Maximum columns",
                "description": "Css libraries (such as twitter bootstrap) have a grid system. You can specify the most amount of columns each row can have here.",
                "value":       12
            }
        },

        "colClass": {
            "type":    "string",
            "options": {
                "label":       "Column class",
                "description": "The class to wrap each column",
                "value":       "col-md-%d"
            }
        }


    }
}